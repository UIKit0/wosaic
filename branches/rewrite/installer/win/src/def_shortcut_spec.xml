<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>

<shortcuts>
	<!-- 
		define the following tag if we want to disable the panel
		on unsupported systems.
  <skipIfNotSupported />
  	-->

  <programGroup defaultName="Wosaic" location="applications"/>

  <shortcut
     name="Wosaic"
     target="$INSTALL_PATH\bin\wosaic.jar"
     workingDirectory="$INSTALL_PATH\bin"
     description="A utility to make Mosaics from your photos"
     iconFile="$INSTALL_PATH\icons\logo.ico"
     iconIndex="0"
     initialState="noShow"
     programGroup="yes"
     desktop="$DesktopShortcutCheckboxEnabled"
     applications="no"
     startMenu="no"
     startup="no" />

  <shortcut
     name="Uninstall"
     target="$INSTALL_PATH\Uninstall-launch.bat"
     workingDirectory="$INSTALL_PATH\Uninstaller"
     description="Uninstall Wosaic"
     initialState="noShow"
     programGroup="yes"
     desktop="no"
     applications="no"
     startMenu="no"
     startup="no" />

</shortcuts>
