<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>

<shortcuts>
	<!-- 
		define the following tag if we want to disable the panel
		on unsupported systems.
  <skipIfNotSupported />
  	-->

  <programGroup defaultName="Wosaic" location="applications"/>

  <!--
  	TODO: Create icon files for our shortcuts
  -->
  <shortcut
     name="Wosaic"
     target="$INSTALL_PATH\Wosaic-launch.bat"
     workingDirectory="$INSTALL_PATH"
     description="A utility to make Mosaics from your photos"
     initialState="noShow"
     programGroup="yes"
     desktop="$DesktopShortcutCheckboxEnabled"
     applications="no"
     startMenu="no"
     startup="no" />

  <shortcut
     name="Uninstall"
     target="$INSTALL_PATH\Uninstall-launch.bat"
     workingDirectory="$INSTALL_PATH\Uninstaller"
     description="Uninstall Wosaic"
     initialState="noShow"
     programGroup="yes"
     desktop="no"
     applications="no"
     startMenu="no"
     startup="no" />

</shortcuts>
